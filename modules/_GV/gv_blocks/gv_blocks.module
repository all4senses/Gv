    'Canada VoIP' => 5,
    'Internet Fax' => 6,
    'Hosted PBX Providers' => 7,
    //'Small Business VoIP' => 8,
  );
  
  if (!$page_index = @$pin_article_to[@$map_path_to_type[@$_SERVER['REDIRECT_URL']]]) {
