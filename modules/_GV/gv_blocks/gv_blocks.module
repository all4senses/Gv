  foreach ($header as $header_field_name => $data) {
    $view->set_item($display_name, 'header', $header_field_name, NULL); // Delete field.
  }
  
  $labels_of_fields_to_leave = array(/*'Title', */ /*'Company'*/ /*, 'More Details'*/);
