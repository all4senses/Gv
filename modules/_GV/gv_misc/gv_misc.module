      $ratio = 1 - (($image_height*100)/215)/100;
      $teaser_lenght = 390 + 390*$ratio;
//      dpm('$image_height = ' . $image_height);
//      dpm('$ratio = ' . $ratio);
//      dpm('$teaser_lenght = ' . $teaser_lenght);
    }
