      $subpath .= '/' . $service_type;
    }
    return '<a ' . ($a_title ? 'title="' . $a_title . '" ' : ''). 'href="/go' . $subpath . '" target = "_blank"' . ($a_attr ? ' ' . $a_attr['key'] . '="' . $a_attr['value'] . '"' : ' rel="nofollow"') . ($a_class ? ' class="' . $a_class . '"' : '') . '>' . $title_or_wrapped_object . '</a>';
  }
  else {
    return NULL;
  }
}


