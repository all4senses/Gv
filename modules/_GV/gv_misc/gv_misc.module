function gv_misc_refineServiceTypeKey($source_key) {
  if ($source_key == 'residential') {
    return 're';
  }
  elseif ($source_key == 'business') {
    return 'bu';
  }
  else {
    return $source_key;
  }
}


function gv_misc_refineServiceTypeKey_Reverse($source_key) {
  if ($source_key == 're') {
    return 'residential';
  }
  elseif ($source_key == 'bu') {
    return 'business';
  }
  else {
    return $source_key;
  }
}


